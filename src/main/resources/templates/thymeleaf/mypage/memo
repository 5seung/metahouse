function parseFormattedNumber(value) {
  // 쉼표 제거
  var cleanedValue = value.replace(/,/g, '');
  // 정수로 파싱
  var parsedValue = parseInt(cleanedValue, 10);
  return parsedValue;
}

var formattedNumber = "555,111,000";
var parsedNumber = parseFormattedNumber(formattedNumber);
console.log(parsedNumber); // 출력: 555111000